{
  "rewrites": [
    { "source": "/api/:path*", "destination": "http://ltc.kagafonov.beget.tech:8100/api/:path*" },
    { "source": "/(.*)", "destination": "/" }
  ],
  "headers": [
    {
      "source": "/regions-simplified.patched.geojson",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }]
    }
  ]
}
